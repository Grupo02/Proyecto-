<Window x:Class="LoginVotante"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoWPF"
        mc:Ignorable="d"
        Title="VOTANTE" Height="250" Width="300" Loaded="Window_Loaded" Icon="img/cne.jpg" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="lblCedVotante" Content="Nro. Cedula" HorizontalAlignment="Left" Margin="34,64,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnIngresarVotante" Content="Ingresar" HorizontalAlignment="Left" Margin="160,143,0,0" VerticalAlignment="Top" Width="75" BorderBrush="{x:Null}" Foreground="#FF3C3C3C">
            <Button.Background>
                <SolidColorBrush Color="#FF3075B9" Opacity="0.3"/>
            </Button.Background>
        </Button>
        <TextBox x:Name="txtCedula" HorizontalAlignment="Left" Height="23" Margin="140,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
