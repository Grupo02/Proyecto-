         xmlns:local="clr-namespace:ProyectoWPF"
         mc:Ignorable="d"
         Title="Resultados de su dignidad" Height="275" Width="300" Icon="img/cne.jpg" Closed="Window_Closed" WindowStartupLocation="CenterScreen">
    
