        <ProgressBar HorizontalAlignment="Left" Height="14" Margin="195,63,0,0" VerticalAlignment="Top" Width="65"/>
        <ProgressBar HorizontalAlignment="Left" Height="14" Margin="195,63,0,0" VerticalAlignment="Top" Width="65"/>
        <ProgressBar HorizontalAlignment="Left" Height="14" Margin="195,93,0,0" VerticalAlignment="Top" Width="65"/>
        <ProgressBar HorizontalAlignment="Left" Height="14" Margin="195,122,0,0" VerticalAlignment="Top" Width="65"/>
        <Button x:Name="btnCerrarSesionCandidato" Content="Cerrar Sesión" HorizontalAlignment="Left" Height="27" Margin="109,191,0,0" VerticalAlignment="Top" Width="81" Background="#FFDC4F4F" Foreground="White"/>

