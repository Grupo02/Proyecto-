<Window x:Class="LoginCandidato"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoWPF"
        mc:Ignorable="d"
        Title="CANDIDATO" Height="300" Width="300">
    <Grid>
        <Label x:Name="lblUserCandidato" Content="Usuario" HorizontalAlignment="Left" Margin="58,73,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUserCandidato" HorizontalAlignment="Left" Height="23" Margin="131,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblClaveCandidato" Content="Clave" HorizontalAlignment="Left" Margin="58,132,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtClaveCandidato" HorizontalAlignment="Left" Height="23" Margin="131,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnIngresarCandidato" Content="Ingresar" HorizontalAlignment="Left" Margin="154,199,0,0" VerticalAlignment="Top" Width="75" Background="#FFB6D2FB" BorderBrush="{x:Null}" Foreground="#FF343333"/>

    </Grid>
</Window>
