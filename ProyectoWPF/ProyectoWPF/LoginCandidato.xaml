<Window x:Class="LoginCandidato"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoWPF"
        mc:Ignorable="d"
        Title="CANDIDATO" Height="300" Width="300" Loaded="Window_Loaded" Icon="img/cne.jpg" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="lblUserCandidato" Content="Usuario" HorizontalAlignment="Left" Margin="60,64,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblClaveCandidato" Content="Clave" HorizontalAlignment="Left" Margin="60,127,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUserCandidato" HorizontalAlignment="Left" Height="23" Margin="136,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnIngresarCandidato" Content="Ingresar" HorizontalAlignment="Left" Margin="167,206,0,0" VerticalAlignment="Top" Width="75" BorderBrush="{x:Null}" Foreground="#FF303030">
            <Button.Background>
                <SolidColorBrush Color="#FF3075B9" Opacity="0.3"/>
            </Button.Background>
        </Button>
        <PasswordBox x:Name="passCandidato" HorizontalAlignment="Left" Margin="136,135,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
