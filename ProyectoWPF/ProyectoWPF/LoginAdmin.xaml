<Window x:Class="LoginAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoWPF"
        mc:Ignorable="d"
        Title="ADMINISTRADOR" Height="300" Width="300">
    <Grid>
        <Label x:Name="lblUserAdmin" Content="Usuario" HorizontalAlignment="Left" Margin="47,69,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUserAdmin" HorizontalAlignment="Left" Height="23" Margin="133,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblClaveAdmin" Content="Clave" HorizontalAlignment="Left" Margin="53,136,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtClave" HorizontalAlignment="Left" Height="23" Margin="133,136,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnIngresarAdmin" Content="Ingresar" HorizontalAlignment="Left" Margin="155,203,0,0" VerticalAlignment="Top" Width="75" Background="#FFB6D2FB" Foreground="#FF363434" BorderBrush="{x:Null}"/>

    </Grid>
</Window>
